<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginBottom="@dimen/dimen_20dp"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="10dp"
            android:weightSum="2">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Mall"
                android:textStyle="bold"
                android:layout_marginBottom="8dp"
                android:textColor="@android:color/black"/>
            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text=">>"
                android:textStyle="bold"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="8dp"
                android:visibility="gone"
                android:gravity="right" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/h1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:layout_marginBottom="16dp"
            android:weightSum="5"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/b1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/id_circle1"
                    android:onClick="onClickCategory"
                    android:layout_width="@dimen/circle_button"
                    android:layout_height="@dimen/circle_button"
                    android:layout_gravity="center_horizontal"
                    android:src="@drawable/genteng"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/circle_button_text"
                    android:layout_gravity="center_horizontal"
                    android:text="Genteng"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/b2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/id_circle2"
                    android:onClick="onClickCategory"
                    android:layout_width="@dimen/circle_button"
                    android:layout_height="@dimen/circle_button"
                    android:layout_gravity="center_horizontal"
                    android:src="@drawable/semen"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/circle_button_text"
                    android:layout_gravity="center_horizontal"
                    android:text="Semen"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/b5"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/id_circle3"
                    android:onClick="onClickCategory"
                    android:layout_width="@dimen/circle_button"
                    android:layout_height="@dimen/circle_button"
                    android:layout_gravity="center_horizontal"
                    android:src="@drawable/batako"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/circle_button_text"
                    android:layout_gravity="center_horizontal"
                    android:text="Batako"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/b6"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/id_circle4"
                    android:onClick="onClickCategory"
                    android:layout_width="@dimen/circle_button"
                    android:layout_height="@dimen/circle_button"
                    android:layout_gravity="center_horizontal"
                    android:src="@drawable/pasir"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/circle_button_text"
                    android:layout_gravity="center_horizontal"
                    android:text="Pasir"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/b4"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/id_circle5"
                    android:onClick="onClickCategory"
                    android:layout_width="@dimen/circle_button"
                    android:layout_height="@dimen/circle_button"
                    android:layout_gravity="center_horizontal"
                    android:src="@drawable/kerikil"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/circle_button_text"
                    android:layout_gravity="center_horizontal"
                    android:text="Kerikil"/>

            </LinearLayout>

        </LinearLayout>


        <LinearLayout
            android:id="@+id/h2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:layout_marginBottom="16dp"
            android:weightSum="5"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/b11"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/id_circle6"
                    android:onClick="onClickCategory"
                    android:layout_width="@dimen/circle_button"
                    android:layout_height="@dimen/circle_button"
                    android:layout_gravity="center_horizontal"
                    android:src="@drawable/alatbangunan"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/circle_button_text"
                    android:layout_gravity="center_horizontal"
                    android:text="Alat Bangunan"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/b22"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/id_circle7"
                    android:onClick="onClickCategory"
                    android:layout_width="@dimen/circle_button"
                    android:layout_height="@dimen/circle_button"
                    android:layout_gravity="center_horizontal"
                    android:src="@drawable/paralon"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/circle_button_text"
                    android:layout_gravity="center_horizontal"
                    android:text="Paralon"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/b55"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/id_circle8"
                    android:onClick="onClickCategory"
                    android:layout_width="@dimen/circle_button"
                    android:layout_height="@dimen/circle_button"
                    android:layout_gravity="center_horizontal"
                    android:src="@drawable/kayu"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/circle_button_text"
                    android:layout_gravity="center_horizontal"
                    android:text="Kayu"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/b66"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/id_circle9"
                    android:onClick="onClickCategory"
                    android:layout_width="@dimen/circle_button"
                    android:layout_height="@dimen/circle_button"
                    android:layout_gravity="center_horizontal"
                    android:src="@drawable/cat"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/circle_button_text"
                    android:layout_gravity="center_horizontal"
                    android:text="Cat Tembok"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/b44"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/id_circle10"
                    android:onClick="onClickCategory"
                    android:layout_width="@dimen/circle_button"
                    android:layout_height="@dimen/circle_button"
                    android:layout_gravity="center_horizontal"
                    android:src="@drawable/lain"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/circle_button_text"
                    android:layout_gravity="center_horizontal"
                    android:text="Lain-lain"/>

            </LinearLayout>


        </LinearLayout>

    </LinearLayout>

</LinearLayout>
