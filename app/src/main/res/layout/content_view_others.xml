<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">

    <com.libRG.CustomTextView
        android:id="@+id/id_dekat_swimpool"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        android:text="Ada kolam renang"
        app:lib_checkedIconPosition="left"
        app:lib_setChecked="true"
        android:enabled="false"
        android:textColor="@color/black"
        app:lib_setCheckedText="true" />

    <com.libRG.CustomTextView
        android:id="@+id/id_dekat_sekolah"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        android:text="Dekat Sekolah"
        app:lib_checkedIconPosition="left"
        app:lib_setChecked="true"
        android:enabled="false"
        android:textColor="@color/black"
        app:lib_setCheckedText="true" />

    <com.libRG.CustomTextView
        android:id="@+id/id_dekat_jalanraya"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        android:text="Dekat Jalan Raya"
        app:lib_checkedIconPosition="left"
        app:lib_setChecked="true"
        android:enabled="false"
        android:textColor="@color/black"
        app:lib_setCheckedText="true" />

    <com.libRG.CustomTextView
        android:id="@+id/id_dekat_toll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        android:text="Dekat Jalan Toll"
        app:lib_checkedIconPosition="left"
        app:lib_setChecked="true"
        android:enabled="false"
        android:textColor="@color/black"
        app:lib_setCheckedText="true" />

    <com.libRG.CustomTextView
        android:id="@+id/id_dekat_pasar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        android:layout_marginTop="4dp"
        android:text="Dekat Pusat Perbelanjaan"
        app:lib_checkedIconPosition="left"
        app:lib_setChecked="true"
        android:enabled="false"
        android:textColor="@color/black"
        app:lib_setCheckedText="true" />

    <com.libRG.CustomTextView
        android:id="@+id/id_dekat_ibadah"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        android:layout_marginTop="4dp"
        android:text="Dekat Tempat Ibadah"
        app:lib_checkedIconPosition="left"
        app:lib_setChecked="true"
        android:enabled="false"
        android:textColor="@color/black"
        app:lib_setCheckedText="true" />

    <com.libRG.CustomTextView
        android:id="@+id/id_dekat_kesehatan"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        android:layout_marginTop="4dp"
        android:text="Dekat Fasilitas Kesehatan"
        app:lib_checkedIconPosition="left"
        app:lib_setChecked="true"
        android:enabled="false"
        android:textColor="@color/black"
        app:lib_setCheckedText="true" />

    <com.libRG.CustomTextView
        android:id="@+id/id_dekat_perumahan"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        android:layout_marginTop="4dp"
        android:text="Dekat Perumahan Umum"
        app:lib_checkedIconPosition="left"
        app:lib_setChecked="true"
        android:enabled="false"
        android:textColor="@color/black"
        app:lib_setCheckedText="true" />

    <com.libRG.CustomTextView
        android:id="@+id/id_dekat_wisata"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp"
        android:layout_marginBottom="12dp"
        android:layout_marginTop="4dp"
        android:text="Dekat Tempat Wisata"
        app:lib_checkedIconPosition="left"
        app:lib_setChecked="true"
        android:enabled="false"
        android:textColor="@color/black"
        app:lib_setCheckedText="true" />

</LinearLayout>
